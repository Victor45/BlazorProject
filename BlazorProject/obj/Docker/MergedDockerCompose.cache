name: dockercompose6501841778222264494
services:
  blazorproject:
    build:
      context: D:\Practicaa\BlazorProject
      dockerfile: BlazorProject/BlazorProject/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: blazorproject
    networks:
      default: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
